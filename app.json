{
  "expo": {
    "name": "EDCMS Mobile",
    "slug": "edcms-mobile",
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/logo/app-logo-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "edcmsmobile",
    "splash": {
      "image": "./assets/logo/app-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Aplikasi ini menggunakan lokasi Anda untuk memberikan layanan yang lebih baik.",
        "NSLocationAlwaysUsageDescription": "Aplikasi ini menggunakan lokasi Anda untuk memberikan layanan yang lebih baik.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Aplikasi ini menggunakan lokasi Anda untuk memberikan layanan yang lebih baik.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.FOREGROUND_SERVICE_DATA_SYNC",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/app-logo-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.app.edcmsmobile",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "edcmsmobile"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/logo/app-logo-icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      // "expo-background-fetch",
      [
        "expo-notifications",
        {
          "icon": "./assets/logo/app-logo-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false,
          "iosDisplayInForeground": true
        }
      ],
      [
        "expo-media-library",
        {
          "savePhotosPermission": "Izinkan $(PRODUCT_NAME) untuk menyimpan foto."
        }
      ],
      // [
      //   "react-native-radar",
      //   {
      //     "iosFraud": true,
      //     "iosNSLocationWhenInUseUsageDescription": "Aplikasi ini menggunakan lokasi Anda untuk memberikan layanan yang lebih baik.",
      //     "iosNSLocationAlwaysAndWhenInUseUsageDescription": "Aplikasi ini menggunakan lokasi Anda untuk memberikan layanan yang lebih baik.",
      //     "iosBackgroundMode": true,
      //     "androidFraud": true,
      //     "androidBackgroundPermission": true,
      //     "androidFineLocationPermission": true,
      //     "addRadarSDKMotion": true,
      //     "iosNSMotionUsageDescription": "Aplikasi ini menggunakan gerakan Anda untuk memberikan layanan yang lebih baik."
      //   }
      // ],
      "expo-router",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "b2171288-6b4a-440c-a8e4-62de4bb46524"
      }
    }
  }
}