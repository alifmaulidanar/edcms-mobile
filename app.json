{
  "expo": {
    "name": "Trackify",
    "slug": "trackify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/trackify-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/trackify-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/trackify-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alifmaulidanar.trackify"
    },
    "web": {
      "favicon": "./assets/trackify-logo.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ]
    ]
  }
}